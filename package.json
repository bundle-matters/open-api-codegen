{
  "name": "openapi-cg",
  "description": "Generate code from an OpenAPI spec",
  "keywords": [
    "openapi",
    "swagger",
    "code",
    "generator",
    "typescript",
    "javascript",
    "axios",
    "node",
    "client",
    "server"
  ],
  "main": "lib/index.js",
  "scripts": {
    "compile": "tsc",
    "lint": "eslint src",
    "test": "vitest",
    "coverage": "c8 --reporter=lcov vitest run --coverage",
    "lint-staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bundle-matters/openapi-cg.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bundle-matters/openapi-cg/issues"
  },
  "homepage": "https://github.com/bundle-matters/openapi-cg#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/adm-zip": "^0.5.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "c8": "^7.11.2",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "semantic-release": "^19.0.2",
    "typescript": "^4.6.4",
    "vitest": "^0.10.2"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{ts,js,css,md}": "prettier --write"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "fs-extra": "^11.1.1",
    "minimatch": "^9.0.3",
    "node-fetch": "^3.3.2"
  }
}
